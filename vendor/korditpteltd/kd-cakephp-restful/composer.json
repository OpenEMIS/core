{
    "name": "korditpteltd/kd-cakephp-restful",
    "description": "KORDIT CakePHP Restfull Plugin",
    "type": "cakephp-plugin",
    "keywords": ["kordit", "cakephp", "restful"],
    "homepage": "https://bitbucket.org/korditpteltd/kd-cakephp-restful",
    "license": "proprietary",
    "authors": [
        {
            "name": "KORDIT PHP DEVELOPERS",
            "homepage": "https://bitbucket.org/korditpteltd/kd-cakephp-restful/commits/all"
        }
    ],
    "support": {
        "source": "https://bitbucket.org/korditpteltd/kd-cakephp-restful/src"
    },
    "require": {
        "cakephp/cakephp": ">3.0.9 <3.2.7"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Restful\\": "src",
            "Restful\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Restful\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "master": "1.0.1-dev"
        }
    },
    "minimum-stability": "beta"
}
