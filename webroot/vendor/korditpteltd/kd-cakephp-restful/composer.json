{
    "name": "korditpteltd/kd-cakephp-restful",
    "description": "KORDIT CakePHP Restful Plugin",
    "type": "cakephp-plugin",
    "keywords": ["kordit", "cakephp", "restful"],
    "homepage": "https://bitbucket.org/korditpteltd/kd-cakephp-restful",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "KORDIT PHP DEVELOPERS",
            "homepage": "https://bitbucket.org/korditpteltd/kd-cakephp-restful/commits/all"
        }
    ],
    "support": {
        "source": "https://bitbucket.org/korditpteltd/kd-cakephp-restful/src"
    },
    "require": {
        "cakephp/cakephp": ">=3.3.0 <3.4",
        "admad/cakephp-jwt-auth": "^2.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^2.3",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Restful\\": "src",
            "Restful\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Restful\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "master": "1.0.2"
        }
    },
    "minimum-stability": "beta"
}
